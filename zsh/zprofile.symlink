# mattman  .dotfiles/zsh/zprofile.symlink

# set up rc_dirs
[[ -s ${ZDOTDIR:-$HOME}/.dotfiles/zsh/rcconfig.zsh ]] && source ${ZDOTDIR:-$HOME}/.dotfiles/zsh/rcconfig.zsh

# init zprezto:zprofile
zprezto_file=${ZDOTDIR:-$HOME}/.zprezto/runcoms/zprofile
source_file "$zprezto_file"

path=(
  $HOME/bin
  $path
)

export EDITOR=vim
export VISUAL=vim

# process any extra env config
for dir in $rc_dirs; do
  source_file "$dir/setenv.zsh"
done


